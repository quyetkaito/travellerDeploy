s.items; //2.remove hết các class active của item khác\n\n      for (var i = 0; i < ref.length; i++) {\n        ref[i].removeActive();\n      } //3.active item hiện tại\n\n\n      ref[index].active();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,aAAP,MAA0B,oCAA1B;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,eAAe;EACbC,UAAU,EAAE;IACV,mBAAmBF;EADT,CADC;;EAIbG,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,CACX;QACEC,IAAI,EAAE,WADR;QAEEC,IAAI,EAAE,qBAFR;QAGEC,IAAI,EAAE;MAHR,CADW,EAMX;QACEF,IAAI,EAAE,UADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CANW,EAWX;QACEF,IAAI,EAAE,UADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CAXW,EAgBX;QACEF,IAAI,EAAE,MADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CAhBW,EAqBV;QACCF,IAAI,EAAE,qBADP;QAECC,IAAI,EAAE,gBAFP;QAGCC,IAAI,EAAE;MAHP,CArBU,EA0BX;QACEF,IAAI,EAAE,oBADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CA1BW,EA+BX;QACEF,IAAI,EAAE,mBADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CA/BW,EAoCX;QACEF,IAAI,EAAE,kBADR;QAEEC,IAAI,EAAE,gBAFR;QAGEC,IAAI,EAAE;MAHR,CApCW,CADR;MA2CLC,OAAO,EAAE;QACPC,KAAK,EAAE;MADA;IA3CJ,CAAP;EA+CD,CApDY;;EAqDbC,OAAO,EAAE;IACP;;;IAGAC,MAAM,GAAE;MACJV,MAAM,CAACW,IAAP,CAAY;QAACC,IAAI,EAAC;MAAN,CAAZ;IACH,CANM;;IAQP;;;;IAIAC,WAAW,GAAG;MACZ,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,iBAAlB,GAAsC,KAAtC;IACD,CAdM;;IAgBP;;;;;IAKAC,YAAY,CAACC,KAAD,EAAQ;MAClB,MAAMC,EAAC,GAAI,IAAX,CADkB,CAElB;;MACA,IAAIC,GAAE,GAAID,EAAE,CAACE,KAAH,CAASC,KAAnB,CAHkB,CAIlB;;MACA,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAIH,GAAG,CAACI,MAAxB,EAAgCD,CAAC,EAAjC,EAAqC;QACnCH,GAAG,CAACG,CAAD,CAAH,CAAOE,YAAP;MACF,CAPkB,CAQlB;;;MACAL,GAAG,CAACF,KAAD,CAAH,CAAWQ,MAAX;IACD;;EA/BM;AArDI,CAAf","names":["MSSidebarItem","router","components","data","sidebarItem","text","icon","href","myStyle","width","methods","goHome","push","path","openSideBar","$store","state","isSideBarCollapse","handleActive","index","me","ref","$refs","items","i","length","removeActive","active"],"sourceRoot":"","sources":["D:\\Traveller\\Traveller-Frontend\\traveller-ui\\src\\layout\\sidebar\\TheSidebar.vue"]